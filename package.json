{
  "name": "eslint-config-everqin",
  "description": "Everqin Team ESLint Rules",
  "version": "0.0.3",
  "author": "handpear<handpear@outlook.com>",
  "license": "MIT",
  "main": "index.js",
  "files": [
    "index.js",
    "react.js",
    "vue.js",
    "typescript.js"
  ],
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-config-everqin",
    "everqin"
  ],
  "homepage": "https://github.com/Handpear/eslint-config-everqin.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/Handpear/eslint-config-everqin.git"
  },
  "bugs": {
    "url": "https://github.com/Handpear/eslint-config-everqin/issues"
  },
  "packageManager": "pnpm@8.1.0",
  "engines": {
    "node": ">=16",
    "pnpm": ">=8"
  },
  "scripts": {
    "build": "ts-node scripts/build.ts",
    "release": "pnpm publish --no-git-checks",
    "test": "pnpm lint && ts-node ./rules/test.ts",
    "type:check": "tsc --noEmit --skipLibCheck",
    "docs:dev": "vitepress dev",
    "docs:build": "vitepress build --base /eslint-docs/",
    "docs:preview": "vitepress preview",
    "clean": "pnpm clean:lib",
    "clean:lib": "rimraf -g **/node_modules",
    "lint": "pnpm lint:eslint && pnpm lint:prettier",
    "lint:eslint": "eslint --ext .js,.jsx,.mjs,.cjs,.ts,.tsx,.mts,.cts,.vue,.md --ignore-pattern \"bad.*\" .",
    "lint:prettier": "prettier -l \"**/*.{js,jsx,mjs,cjs,ts,tsx,mts,cts,css,less,scss,json,vue,html,md}\"",
    "lib:up": "taze -I -r --exclude pnpm",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/preset-react": "^7.18.6",
    "@types/doctrine": "^0.0.5",
    "@types/eslint": "^8.37.0",
    "@types/node": "^18.16.2",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "doctrine": "^3.0.0",
    "eslint": "^8.39.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-vue": "^9.11.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "rimraf": "^5.0.0",
    "taze": "^0.9.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vitepress": "1.0.0-alpha.74",
    "vue": "^3.2.47",
    "vue-eslint-parser": "^9.1.1"
  },
  "peerDependencies": {
    "@babel/eslint-parser": "7.x",
    "@babel/preset-react": "7.x",
    "@typescript-eslint/eslint-plugin": ">=5.56.0",
    "@typescript-eslint/parser": "5.x",
    "eslint": ">=8.36.0",
    "eslint-plugin-react": ">=7.32.0",
    "eslint-plugin-vue": ">=9.9.0",
    "typescript": ">=4.7.4",
    "vue-eslint-parser": "9.x"
  },
  "peerDependenciesMeta": {
    "@babel/eslint-parser": {
      "optional": true
    },
    "@babel/preset-react": {
      "optional": true
    },
    "@typescript-eslint/eslint-plugin": {
      "optional": true
    },
    "@typescript-eslint/parser": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "typescript": {
      "optional": true
    },
    "vue-eslint-parser": {
      "optional": true
    }
  }
}
